<publication>
  <source>
    <directories external="../assets" generated="../generated-assets-ed2" />
    <version include="ed2 static ed2-static" />
  </source>
  <html>
    <css theme="default-modern" palette="blues" />
    <!-- Examples and exercises are knowlized by default -->
    <!-- We disable this behavior  -->
    <knowl example="no" exercise-divisional="no" exercise-inline="no"
	   />
    <webwork divisional="dynamic" />
    <feedback href="http://gvsu.edu/s/0vK" />
  </html>
  <latex sides="two" print="yes" font-size="10" pageref="no">
    <page>
      <geometry>paperwidth=7.44in,paperheight=9.69in,tmargin=.5in,bmargin=.3in,hmargin=.75in,bindingoffset=.4in,includeheadfoot,marginparsep=6ex</geometry>
    </page>
    <worksheet formatted="no"/>
  </latex>
  <common>
    <!-- List Chapters and Sections in sidebar Table of Contents -->
    <tableofcontents level="2" />
    <exercise-divisional statement="yes" hint="no" answer="no" solution="no"/>
    <exercise-inline statement="yes" hint="yes" answer="no" solution="no"/>
    <exercise-project statement="yes" hint="no" answer="no" solution="no" />
    <exercise-worksheet statement="yes" hint="no" answer="no" solution="no" />
    <worksheet left="13mm" right="13mm" top="13mm" bottom="13mm" />
  </common>
  <webwork server="https://webwork-dev.uleth.ca"
           course="chrissy-playground" password="anonymous"
           user="anonymous"
           task-reveal="all"
    />
</publication>
